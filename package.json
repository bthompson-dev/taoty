{
  "name": "taoty",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.1.3",
    "moment": "^2.29.4",
    "mongoose": "^6.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile:sass": "sass src/sass/main.scss src/App.css",
    "watch:sass": "sass --watch src/sass/main.scss src/App.css",
    "deploy:build": "npm run build && cp -r build/. ../'taoty - build'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "sass": "^1.55.0"
  }
}
